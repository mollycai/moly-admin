{
  "name": "backstage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "test": "vite --mode test",
    "build": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "ts": "vue-tsc --noEmit",
    "eslint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.{js,ts,json,jsx,tsx,css,less,scss,vue,html,md}\""
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "0.27.2",
    "element-plus": "^2.7.5",
    "lodash-es": "4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/node": "^20.12.12",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "8.39.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.11.0",
    "husky": "8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.2.4",
    "sass": "^1.77.4",
    "typescript": "5.2.2",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  },
  "lint-staged": {
    "src/**/*.{vue,js,jsx,ts,tsx,json}": [
      "yarn run lint",
      "prettier --write"
    ]
  }
}
